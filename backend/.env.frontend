# Frontend Environment Variables for Component Generator
# Use this for your React/Next.js frontend application

# Backend API URL (your Vercel backend)
REACT_APP_API_URL=https://ai-code-generator-vhty.vercel.app
NEXT_PUBLIC_API_URL=https://ai-code-generator-vhty.vercel.app

# API Version
REACT_APP_API_VERSION=v1
NEXT_PUBLIC_API_VERSION=v1

# WebSocket URL (if using WebSocket for real-time features)
REACT_APP_WS_URL=wss://ai-code-generator-vhty.vercel.app
NEXT_PUBLIC_WS_URL=wss://ai-code-generator-vhty.vercel.app

# App Configuration
REACT_APP_APP_NAME=Component Generator
NEXT_PUBLIC_APP_NAME=Component Generator

# Environment
NODE_ENV=production
NEXT_PUBLIC_NODE_ENV=production

# Google Analytics (optional)
REACT_APP_GA_TRACKING_ID=your_ga_tracking_id_here
NEXT_PUBLIC_GA_TRACKING_ID=your_ga_tracking_id_here

# Sentry Error Tracking (optional)
REACT_APP_SENTRY_DSN=your_sentry_dsn_here
NEXT_PUBLIC_SENTRY_DSN=your_sentry_dsn_here

# Feature Flags (optional)
REACT_APP_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_ANALYTICS=true

# For Vercel deployment
VERCEL=1
VERCEL_URL=$VERCEL_URL
